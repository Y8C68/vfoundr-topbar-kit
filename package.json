{
  "name": "@y8c68/topbar-kit",
  "version": "0.2.1",
  "description": "Shared top bar component for VFoundr applications",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.mjs",
      "require": "./dist/hooks/index.js"
    },
    "./lib": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.mjs",
      "require": "./dist/lib/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vfoundr",
    "topbar",
    "navigation",
    "react",
    "component"
  ],
  "author": "VFoundr",
  "license": "MIT",
  "peerDependencies": {
    "@clerk/clerk-react": "^4.0.0 || ^5.0.0",
    "lucide-react": "^0.263.0 || ^0.300.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0"
  },
  "dependencies": {
    "@y8c68/topbar-kit": "^0.1.0",
    "clsx": "^2.0.0"
  },
  "devDependencies": {
    "@clerk/clerk-react": "^5.35.3",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "lucide-react": "^0.300.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.30.1",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
